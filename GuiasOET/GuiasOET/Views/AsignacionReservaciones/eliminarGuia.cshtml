@model GuiasOET.Models.GUIA_EMPLEADO

@{
    Layout = null;
}

<tr id="asignacion_@(ViewBag.rowCount)">
    <td>
        @Html.DisplayFor(modelItem => Model.GUIA_EMPLEADO.NOMBREEMPLEADO)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.GUIA_EMPLEADO.NOMBREESTACION)
    </td>
    <td>
        @Html.DisplayFor(modelItem => Model.GUIA_EMPLEADO.TIPOEMPLEADO)
    </td>
    <td>
        @Html.ActionLink("Asignar", "AsignarReservacionDetallada", null, new { @class = "btn btn-default", @onclick = "agregarGuia(" + Model.guiasDisponibles.ElementAt(i).CEDULA + "," + ViewBag.reserva + ",$('#asignacion_" + i + "'))", @style = "background-color: #7BC143; color:white;" })
    </td>
</tr>